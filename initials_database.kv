<Initials_database>:
	canvas.before:
		Color:
			rgba: 0.909,.898,1,1
		Rectangle:
			pos: self.pos
			size: self.size
	padding: 20
	spacing: 20
	Button:
		text: "Go Back!"
		#background_color: 0,0,0,0
		markup: True
		#pos_hint: {'x': .35, 'y': .55}
		#size_hint: .3,.4
		font_size: 24
		on_release: root.on_back()
		
	Button:
		
		#background_color: 0,0,0,0
		#pos_hint: {'x':.35 , 'y': .10}
		#size_hint: .3,.4
		on_release: root.on_poe()
		BoxLayout:
					
			orientation: 'vertical'
			pos: self.parent.pos
			size: self.parent.size
			Image:
				source: 'ic_action_edit.png'
			Label:
				text: 'Enter POE'
				font_size: 24
				markup: True